extends layouts/login-layout

block content
    .loginContainer 
        h1 Login 
        form( method="post")
            input(type="text", name="logUsername", placeholder = "Username/E-mail", required ="")
            input(type="password", name="logPassword", placeholder = "Password", required ="")
            input(type="submit", class="btn" value="Login")
        a(href="/register") Not signed up? Register Here.
        
        
